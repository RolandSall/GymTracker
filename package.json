{
  "name": "@gym-tracker/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "backend:dev": "nx run-many --target=build --projects=nest-mediator && nx serve backend --skip-nx-cache",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "db:generate": "drizzle-kit generate --config=apps/backend/src/config/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=apps/backend/src/config/drizzle.config.ts",
    "db:push": "drizzle-kit push --config=apps/backend/src/config/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=apps/backend/src/config/drizzle.config.ts",
    "db:seed": "tsx apps/backend/src/config/db/seed.ts"
  },
  "private": true,
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.9",
    "@nx/js": "21.6.4",
    "@nx/nest": "^21.6.4",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "20.19.9",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.31.5",
    "nx": "21.6.4",
    "tslib": "^2.3.0",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2"
  },
  "workspaces": [
    "packages/*",
    "apps/*",
    "libs/*"
  ],
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "uuid": "^13.0.0"
  }
}