{
  "name": "@gym-tracker/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "frontend:dev": "nx serve frontend",
    "frontend:build": "nx build frontend",
    "frontend:build:prod": "nx build frontend --configuration=production",
    "frontend:test": "nx test frontend",
    "backend:dev": "NODE_ENV=development nx serve backend",
    "backend:prod": "NODE_ENV=production nx serve backend",
    "backend:e2e": "NODE_ENV=development nx build backend && node dist/apps/backend/main.js",
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "db:generate": "drizzle-kit generate --config=apps/backend/config/drizzle-dev.config.ts --name=$npm_config_name",
    "db:migrate:dev": "drizzle-kit migrate --config=apps/backend/config/drizzle-dev.config.ts",
    "db:migrate:prod": "drizzle-kit migrate --config=apps/backend/config/drizzle-prod.config.ts",
    "db:push:dev": "drizzle-kit push --config=apps/backend/config/drizzle-dev.config.ts",
    "db:push:prod": "drizzle-kit push --config=apps/backend/config/drizzle-prod.config.ts",
    "db:studio:dev": "drizzle-kit studio --config=apps/backend/config/drizzle-dev.config.ts",
    "db:studio:prod": "drizzle-kit studio --config=apps/backend/config/drizzle-prod.config.ts",
    "db:seed": "tsx apps/backend/config/db/seed.ts",
    "test:e2e:category": "cd apps/backend/category-management && cucumber-js --config test/e2e/cucumber.json test/e2e/features"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.6",
    "@angular/cli": "^20.3.6",
    "@angular/common": "^20.3.6",
    "@angular/compiler": "^20.3.6",
    "@angular/compiler-cli": "^20.3.6",
    "@angular/core": "^20.3.6",
    "@angular/platform-browser": "^20.3.6",
    "@angular/platform-browser-dynamic": "^20.3.6",
    "@angular/router": "^20.3.6",
    "@cucumber/cucumber": "^12.2.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.9",
    "@nx/angular": "^21.6.5",
    "@nx/js": "21.6.4",
    "@nx/nest": "^21.6.4",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testcontainers/postgresql": "^11.7.1",
    "@types/chai": "^5.2.2",
    "@types/cucumber": "^6.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "20.19.9",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.2",
    "chai": "^6.2.0",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-preset-angular": "^15.0.3",
    "nx": "21.6.4",
    "postcss": "^8.5.6",
    "testcontainers": "^11.7.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.1",
    "@rolandsall24/nest-mediator": "^0.4.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "uuid": "^13.0.0",
    "zone.js": "^0.15.1"
  }
}
